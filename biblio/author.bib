@article{smirnovFizikotehnicheskieProblemyObogashcheniya2020,
	title = {Физико-технические проблемы обогащения регенерированного урана при многократном рецикле в легководных реакторах и пути их решения},
	volume = {128},
	copyright = {All rights reserved},
	issn = {0004-7163},
	url = {https://www.elibrary.ru/item.asp?id=43110962},
	abstract = {В статье обсуждаются вопросы замыкания топливного цикла. Показано, что с точки зрения полного использования регенерированных ядерных материалов основные проблемы связаны с многократным рециклом урана, что обусловлено ограничениями, налагаемыми на содержание изотопа 232U в свежем топливе из регенерированных материалов. В статье выполнена оценка потерь изотопа 235U при многократном рецикле регенерированного урана в виде топлива из регенерата урана и смеси регенерированных оксидов урана и плутония. Показан способ замыкания топливного цикла легководного реактора, который позволяет снизить почти до нуля потери 235U при очистке регенерированного урана от 232U},
	language = {ru},
	number = {4},
	urldate = {2020-08-14},
	journal = {Атомная Энергия},
	author = {Смирнов, А. Ю. and Гусев, В. Е. and Сулаберидзе, Г. А. and Невиница, В. А. and Бобров, Е. А. and Белов, И. А. and Родионова, Е. В.},
	year = {2020},
	note = {Publisher: Редакция журнала "Атомная энергия"},
	file = {Смирнов_et_al_2020_Физико-технические_проблемы_обогащения_регенерированного_урана_при_многократном.pdf:C\:\\Users\\Vlad\\OneDrive\\PhD_bibliography\\Смирнов_et_al_2020_Физико-технические_проблемы_обогащения_регенерированного_урана_при_многократном.pdf:application/pdf}
}

