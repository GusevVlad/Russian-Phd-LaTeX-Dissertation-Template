@article{fedorovANALIZSUShchESTVUYuShchIHSPOSOBOV2020,
  title = {{АНАЛИЗ СУЩЕСТВУЮЩИХ СПОСОБОВ ПОЛУЧЕНИЯ СМЕШАННОГО НИТРИДНОГО УРАН-ПЛУТОНИЕВОГО ТОПЛИВА В РОССИИ И ЗА РУБЕЖОМ}},
  author = {Fedorov, Maxim S. and Baydakov, Nikolay A. and Zhiganov, Alexander N. and Zozulya, Dmitry V.},
  year = {2020},
  month = may,
  journal = {ИЗВЕСТИЯ ВЫСШИХ УЧЕБНЫХ ЗАВЕДЕНИЙ. СЕРИЯ «ХИМИЯ И ХИМИЧЕСКАЯ ТЕХНОЛОГИЯ»},
