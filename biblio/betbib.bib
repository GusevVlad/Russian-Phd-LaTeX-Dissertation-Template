@article{alekseevPossibleScenariosTransition2020,
  title = {Possible Scenarios of Transition to the Closed Nuclear Fuel Cycle},
  author = {Alekseev, P. N. and Gurin, A. V. and Subbotin, S. A. and Zilberman, B. Ya and Kovalev, N. V.},
  year = {2020},
