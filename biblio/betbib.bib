  file = {C\:\\Users\\Vlad\\OneDrive\\PhD_bibliography\\Андрианова_et_al_2015_ПЕРСПЕКТИВНЫЕ_ТОПЛИВНЫЕ_ЗАГРУЗКИ_РЕАКТОРОВ_ДЛЯ_ЗАМКНУТОГО_ТОПЛИВНОГО_ЦИКЛА.pdf}
