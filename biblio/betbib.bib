@article{renvallImagingRealTimeTactile2020,
  title = {Imaging {{Real-Time Tactile Interaction With Two-Person Dual-Coil fMRI}}},
  author = {Renvall, Ville and Kauramäki, Jaakko and Malinen, Sanna and Hari, Riitta and Nummenmaa, Lauri},
