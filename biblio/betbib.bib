@article{alekseevConceptUseRecycled2010,
  title = {The Concept of the Use of Recycled Uranium for Increasing the Degree of Security of Export Deliveries of Fuel for Light-Water Reactors},
  author = {Alekseev, P. N. and Ivanov, E. A. and Nevinitsa, V. A. and {Ponomarev-Stepnoi}, N. N. and Rumyantsev, A. N. and Shmelev, V. M. and Borisevich, V. D. and Smirnov, A. {\relax Yu}. and Sulaberidze, G. A.},
  year = {2010},
